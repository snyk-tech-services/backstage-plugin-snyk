{
  "name": "backstage-plugin-snyk",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "version": "0.0.0-development",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha/index.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "backstage": {
    "role": "frontend-plugin",
    "pluginId": "snyk",
    "pluginPackages": [
      "backstage-plugin-snyk"
    ]
  },
  "scripts": {
    "build": "yarn tsc",
    "start": "backstage-cli package start --config ./dev/app-config.yaml",
    "test": "backstage-cli test"
  },
  "dependencies": {
    "@backstage/catalog-model": "backstage:^",
    "@backstage/core-compat-api": "backstage:^",
    "@backstage/core-components": "backstage:^",
    "@backstage/core-plugin-api": "backstage:^",
    "@backstage/frontend-plugin-api": "backstage:^",
    "@backstage/plugin-catalog-react": "backstage:^",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@snyk/dep-graph": "2.8.1",
    "@types/react-dom": "^18.2.18",
    "react-circular-progressbar": "^2.1.0",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@backstage/cli": "backstage:^",
    "@backstage/dev-utils": "backstage:^",
    "@backstage/test-utils": "backstage:^",
    "@backstage/frontend-defaults": "backstage:^",
    "@backstage/plugin-catalog": "backstage:^",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "msw": "^2.0.5",
    "node-fetch": "^3.3.2",
    "semantic-release": "^24.2.0",
    "react": "^18.0.2",
    "react-dom": "^18.0.2",
    "react-router-dom": "^6.3.0",
    "snyk": "^1.1243.0"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0",
    "react-dom": "^16.13.1 || ^17.0.0",
    "react-router-dom": "^6.18.0"
  },
  "files": [
    "dist",
    "config.d.ts"
  ],
  "configSchema": "config.d.ts",
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ]
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
